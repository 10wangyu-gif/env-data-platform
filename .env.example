# 环境变量示例文件
# 复制为 .env 并修改相应值

# 应用环境
ENV=development
CONFIG_PATH=config/config.dev.yaml

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=env_data_platform
DB_USERNAME=env_user
DB_PASSWORD=env_password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=24h

# 日志配置
LOG_LEVEL=debug
LOG_FORMAT=console
LOG_OUTPUT=both

# 文件上传配置
UPLOAD_MAX_SIZE=100MB
UPLOAD_PATH=uploads

# HJ212协议配置
HJ212_TCP_PORT=8212

# 监控配置
PROMETHEUS_ENABLED=true
HEALTH_CHECK_ENABLED=true